# -*- mode: snippet -*-
# name: flake-sys
# key: flksys
# --
{
  inputs.nixpkgs.url = "nixpkgs";
  outputs = { self, nixpkgs }:
    let
      system = "x86_64-linux";
      pkgs = import nixpkgs {
        config = { allowUnfree = true; };
        localSystem = \${system};
      };
    in {
      devShell.\${system} = with pkgs; mkShell {

      };
    };
}
